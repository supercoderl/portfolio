{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/dist/portfolio"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/frontend/dist/portfolio/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/dist/portfolio/index.html"
    }
  ]
}
